{
  "name": "server-ebd",
  "version": "1.0.0",
  "description": "back-end, ebd platafomr",
  "main": "/dist/main.js",
  "type": "module",
  "scripts": {
    "build": "npx prisma generate && tsc",
    "prisma:deploy": "npx prisma migrate deploy && npx prisma db seed",
    "start": "node dist/main.js",
    "prisma:dev": "npx prisma migrate dev && npx prisma db seed",
    "dev": "tsx watch src/main.ts",
    "typecheck": "npx tsc --watch --noEmit"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "repository": {
    "type": "github",
    "url": ""
  },
  "keywords": [
    "scholl",
    "plataform",
    "sunday-scholl",
    "ebd"
  ],
  "author": "@eduardojpanzo",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.5",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.18.1",
    "@types/swagger-ui-express": "^4.1.8",
    "prisma": "^6.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@types/cors": "^2.8.17",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jose": "^6.0.10",
    "nanoid": "^5.1.5",
    "redis": "^4.7.0",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.24.2"
  },
  "imports": {
    "#app/*": [
      "./dist/app/*"
    ],
    "#core/*": [
      "./dist/core/*"
    ],
    "#infra/*": [
      "./dist/infra/*"
    ],
    "#utils/*": [
      "./dist/utils/*"
    ],
    "#generated/*": [
      "./generated/*"
    ]
  }
}
